#include<stdint.h>
#include<stdio.h>
typedef int (8-t) BYTE;
int main(int argc, char *argc[])
{
    if(arg != 2)
     return 1;
    FILE*file= fopen(argc[1],"r"); // Open file
    if(!file)
     return 1;
    BYTE bytes[3]; //Read 1st three bytes
    fread(bytes,size of (BYTE),3,file);
    // Check 1st three bytes
    if(bytes[0]==0xff && bytes[1]==0xd8 && bytes[2]==0xff)
     printf("Maybe\n");
    else
     printf("No\n");
}
